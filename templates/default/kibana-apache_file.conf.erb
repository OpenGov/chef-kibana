<VirtualHost <%= @params[:listen_address] %>:<%= @params[:listen_port] %>>
  ServerName <%= @params[:server_name] %>

  ServerAlias <%= @params[:server_aliases].join(" ") %>

  ProxyPass / http://0.0.0.0:<%= @kibana_port %>/
  ProxyPassReverse / http://0.0.0.0:<%= @kibana_port %>/
  
  <Location / >
    Require all granted
  </Location>

</VirtualHost>
